{
    "name": "eana-backend-nest",
    "version": "0.0.1",
    "description": "Enterprise NestJS Backend for EANA",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "restore-data": "ts-node scripts/restore-from-csv.ts",
        "bootstrap": "ts-node scripts/bootstrap.ts",
        "bootstrap:prod": "node dist/scripts/bootstrap.js"
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@nestjs/cache-manager": "^2.1.1",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.4.22",
        "@nestjs/platform-socket.io": "^10.4.22",
        "@nestjs/serve-static": "^4.0.2",
        "@nestjs/swagger": "^7.1.17",
        "@nestjs/throttler": "^5.1.1",
        "@nestjs/typeorm": "^10.0.0",
        "@nestjs/websockets": "^10.4.22",
        "@types/nodemailer": "^7.0.5",
        "@types/pdfkit": "^0.17.4",
        "axios": "^1.13.2",
        "bcrypt": "^5.1.1",
        "cache-manager": "^5.2.4",
        "cache-manager-redis-yet": "^4.1.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "compression": "^1.8.1",
        "csv-parse": "^6.1.0",
        "dotenv": "^17.2.3",
        "helmet": "^7.1.0",
        "multer": "^2.0.2",
        "nodemailer": "^7.0.12",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "pdf-parse": "^1.1.1",
        "pdfkit": "^0.17.2",
        "pg": "^8.17.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "socket.io": "^4.8.3",
        "typeorm": "^0.3.28",
        "uuid": "^9.0.1",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/bcrypt": "^5.0.2",
        "@types/compression": "^1.8.1",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.3.1",
        "@types/passport-jwt": "^3.0.13",
        "@types/pdf-parse": "^1.1.5",
        "@types/pg": "^8.16.0",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}